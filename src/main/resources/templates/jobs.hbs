<div class="container-fluid">

    <div class="col-md-12">
        <div class="heading">
            <h1>Get The <span>Right Job</span><br>You Deserve</h1>
            <h6>Your dream job is waiting</h6>

            <div class="search-box">
                <input class="search-txt" type="text" name="" placeholder="Type to search">
                <a class="search-btn" href="#">
                    <i class='bx bx-search-alt-2'></i>
                </a>
            </div>
        </div>
        <div class="container">
            <div class="platform">
                <div class="heading">
                    <h2>One PlatFlorm<br><span>Many Solutions</span></h2>
                </div>
                <div class="row">
                    <div class="col-sm-6">
                        <div class="card">
                            <div class="card-body">
                                <h5 class="card-title">Full stack Developer</h5>
                                <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
                                <a href="#" class="btn btn-primary">Go somewhere</a>
                            </div>
                        </div>
                    </div>
                    <div class="col-sm-6">
                        <div class="card">
                            <div class="card-body">
                                <h5 class="card-title">Android Developer</h5>
                                <p class="card-text">With supporting text below as a natural lead-in to additional content.</p>
                                <a href="#" class="btn btn-primary">Go somewhere</a>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        <h2><span>Featured</span>Jobs</h2>
        {{#each jobs}}
            <div class="card-body">
                <h5 class="card-title">{{title}}</h5>
                <p class="card-text">
                <p>Client's Name: {{company}}</p>
                <p>Client's Email: {{title}}</p>
                <p>Message Title: {{description}}</p>
                <p>Content: {{duration}}</p>
                <p>Content: {{languages}}</p>
                <p class="card-footer">
                <p><a class="btn btn-danger justify-content-left" href="/jobs/{{id}}/delete">delete {{title}}</a> </p>
                <hr>
            </div>
        {{/each}}
